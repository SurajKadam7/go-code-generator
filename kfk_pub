package test

import(
  "github.com/segmentio/kafka-go"
  "context"
  "github.com/go-redis/redis"
  )


type Publish struct{
   client kafka.Client
   next Service
}

func New(client kafka.Client, next Service) Publish{
   return Publish{
      client: client,
      next: next,
   }
}


func (p Publish) Test(ctx context.Context,clietn redis.Client,)(err error,) {
  vr := type Test_Kafka struct{
          ctx context.Context
          clietn redis.Client
          }

  data, err := json.NewEncoder(os.Stdout).Encode(&vr)
  if err != nil{
    return err
  }
  return p.client.Publish()
}

